{%  extends 'layout.twig' %}

{% block content %}
<section style="margin-top:-30px;">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="card border-0 shadow-sm">
                    <div class="card-body">

                        <h2 class="mb-0">OSRS Interactive Map</h2>
                        <p>This was made possible by Explv's OSRS Interactive Map found <a href="https://github.com/Explv/Explv.github.io" target="_blank"> here</a>. ( Opens GitHub repo )</p>
                        <hr>

                        <div class="embed-responsive embed-responsive-16by9">
                             <iframe class="embed-responsive-item" src="https://explv.github.io/" style="border:0px #ffffff none;"
                            name="myiFrame" scrolling="no" frameborder="0" marginheight="0px" marginwidth="0px" height="850px" width="1280px" allowfullscreen>
                            </iframe>
                        </div>

                    </div>
                </div>

                {{ content|raw }}
            </div>



            <div class="scene-container stars-out" tabindex="1">
                <div class="ghost-container">
                  <div class="ghost">
                    <div class="ghost-head">
                      <div class="ghost-face">
                        <div class="eyes-row">
                          <div class="eye left"></div>
                          <div class="eye right"></div>
                        </div>
                        <div class="mouth-row">
                          <div class="cheek left"></div>
                          <div class="mouth">
                            <div class="mouth-top"></div>
                            <div class="mouth-bottom"></div>
                          </div>
                          <div class="cheek right"></div>
                        </div>
                      </div>
                    </div>
                    <div class="ghost-body">
                      <div class="ghost-hand hand-left"></div>
                      <div class="ghost-hand hand-right"></div>
                      <div class="ghost-skirt">
                        <div class="pleat down"></div>
                        <div class="pleat up"></div>
                        <div class="pleat down"></div>
                        <div class="pleat up"></div>
                        <div class="pleat down"></div>
                      </div>
                    </div>
                  </div>
                  <div class="stars">
                    <div class="star orange pointy star-1"><div class="star-element"></div></div>
                    <div class="star orange pointy star-2"><div class="star-element"></div></div>
                    <div class="star yellow pointy star-3"><div class="star-element"></div></div>
                    <div class="star yellow pointy star-4"><div class="star-element"></div></div>
                    <div class="star blue round star-5"><div class="star-element"></div></div>
                    <div class="star blue round star-6"><div class="star-element"></div></div>
                  </div>
                </div>
                <div class="shadow-container">
                  <div class="shadow"></div>
                  <div class="shadow-bottom"></div>
                </div>
              </div>




        </div>
    </div>
</section>

{%  endblock %}
