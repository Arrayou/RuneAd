                                        { x: new Date(2020, 10, 24), y: 13210 },
                                        { x: new Date(2020, 10, 25), y: 14788 },
                                        { x: new Date(2020, 10, 26), y: 16732 },
                                        { x: new Date(2020, 10, 27), y: 14910 },
                                        { x: new Date(2020, 10, 28), y: 14730 },
